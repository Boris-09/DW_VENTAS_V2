{
	"name": "dataflow1",
	"properties": {
		"folder": {
			"name": "Silver"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sql_Clientes",
						"type": "DatasetReference"
					},
					"name": "clientes"
				},
				{
					"dataset": {
						"referenceName": "ds_sql_Clientes_BD_GEN",
						"type": "DatasetReference"
					},
					"name": "BDGEN"
				},
				{
					"dataset": {
						"referenceName": "ds_sql_Clientes_location",
						"type": "DatasetReference"
					},
					"name": "location"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "cast1"
				},
				{
					"name": "cast2"
				},
				{
					"name": "cast3"
				},
				{
					"name": "select1"
				},
				{
					"name": "select2"
				},
				{
					"name": "union1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          CST_ID as string,",
				"          CST_KEY as string,",
				"          CST_FIRSTNAME as string,",
				"          CST_LASTNAME as string,",
				"          CST_MARITAL_STATUS as string,",
				"          CST_GNRD as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> clientes",
				"source(output(",
				"          CID as string,",
				"          BDATE as string,",
				"          GEN as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> BDGEN",
				"source(output(",
				"          CID as string,",
				"          CNTRY as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> location",
				"select1 cast(output(",
				"          BDATE as date 'yyyy-MM-dd'",
				"     ),",
				"     errors: true) ~> cast1",
				"select2 cast(output(",
				"          CNTRY as string",
				"     ),",
				"     errors: true) ~> cast2",
				"clientes cast(output(",
				"          CST_ID as string",
				"     ),",
				"     errors: true) ~> cast3",
				"BDGEN select(mapColumn(",
				"          BDATE",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select1",
				"location select(mapColumn(",
				"          CNTRY",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> select2",
				"cast3, cast1 union(byName: true)~> union1"
			]
		}
	}
}