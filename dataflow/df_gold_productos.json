{
	"name": "df_gold_productos",
	"properties": {
		"folder": {
			"name": "Gold"
		},
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sql_silver_productos",
						"type": "DatasetReference"
					},
					"name": "productos"
				}
			],
			"sinks": [],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          PRD_ID as integer,",
				"          PRD_KEY as string,",
				"          PRD_NM as string,",
				"          PRD_COST as string,",
				"          PRD_LINE as string,",
				"          CAT as string,",
				"          MAINTENANCE as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     query: 'SELECT \\n    p.PRD_KEY,\\n    p.PRD_NM,\\n    p.PRD_COST,\\n    p.PRD_LINE,\\n    pc.CAT,\\n    pc.MAINTENANCE\\nFROM [SILVER].[PRODUCTOS] p\\nLEFT JOIN [BRONZE].[PRODUCTOS_CAT_MANT] pc \\n    ON p.PRD_ID = pc.ID\\nWHERE p.PRD_KEY IS NOT NULL',",
				"     format: 'query') ~> productos"
			]
		}
	}
}